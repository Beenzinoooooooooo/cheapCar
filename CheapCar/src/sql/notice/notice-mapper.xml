<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

	<comment>
		Notice Mapper
	</comment>
	
	
	<entry key="selectNoticeList">
		SELECT
			   NOTICE_NO,
			   NOTICE_TITLE,
			   MEMBER_NAME,
			   CREATE_DATE,
			   COUNT
          FROM 
               NOTICE
          JOIN
               MEMBER ON(MEMBER_NO)
         WHERE
               NOTICE.STATUS = 'Y'
         ORDER
            BY
               NOTICE_NO DESC                           			   
		
	</entry>
	
	
	
	
	
	
	
	
	
	
	

</properties>
